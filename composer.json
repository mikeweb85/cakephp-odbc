{
    "name": "mikeweb85/cakephp-odbc",
    "description": "ODBC plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "ext-pdo": "*",
        "ext-odbc": "*",
        "ext-pdo_odbc": "*",
        "cakephp/cakephp": "^4 || ^5"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "MikeWeb\\CakeOdbc\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MikeWeb\\CakeOdbc\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
